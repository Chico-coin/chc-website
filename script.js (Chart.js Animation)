const ctx = document.getElementById('chcChart');

if (ctx) {
    new Chart(ctx, {
        type: 'line',
        data: {
            labels: ['1H','4H','8H','12H','1D','2D'],
            datasets: [{
                label: 'CHC Price',
                data: [0.1,0.12,0.08,0.11,0.13,0.15],
                borderColor: '#1e90ff',
                borderWidth: 3,
                tension: 0.3,
            }]
        },
        options: {
            responsive: true,
            scales: { y: { beginAtZero: false } }
        }
    });
}
